{
  "name": "cell",
  "version": "1.0.0",
  "description": "Simple LLM agent TUI",
  "main": "dist/cli.js",
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc -p tsconfig.json && node -e \"const fs = require('node:fs'); const file = 'dist/cli.js'; const content = fs.readFileSync(file, 'utf8'); if (!content.startsWith('#!/usr/bin/env node')) { fs.writeFileSync(file, '#!/usr/bin/env node\\n' + content); }\"",
    "start": "node dist/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.27.0",
  "type": "module",
  "bin": {
    "cell": "dist/cli.js"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@mariozechner/pi-agent-core": "^0.50.7",
    "@mariozechner/pi-ai": "^0.50.7",
    "@mariozechner/pi-tui": "^0.50.7",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
